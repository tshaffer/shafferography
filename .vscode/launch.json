{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug shafferography",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/backend/dist/server.js",
      "preLaunchTask": "tsc: build - backend",
      "cwd": "${workspaceFolder}/backend",
      "outFiles": [
        "${workspaceFolder}/backend/dist/**/*.js"
      ],
      "envFile": "${workspaceFolder}/backend/.env",
      "sourceMaps": true
    }
  ]
}
